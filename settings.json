{
  "editor.fontFamily": "MonoLisa",
  "editor.renderWhitespace": "none",
  "editor.snippetSuggestions": "none",
  "editor.fontSize": 14,
  "editor.tabSize": 4,
  "editor.minimap.enabled": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.bracketPairColorization.enabled": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.detectIndentation": false,
  "workbench.startupEditor": "none",
  "workbench.editor.tabCloseButton": "left",
  "workbench.panel.opensMaximized": "always",
  "workbench.layoutControl.enabled": false,
  "workbench.list.horizontalScrolling": true,
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "mlia soft",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.tabs.location": "left",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.cursorBlinking": true,

  "vscode-neovim.neovimInitVimPaths.darwin": "~\/.config\/nvim\/init.vim",
  "vscode-neovim.neovimExecutablePaths.darwin": "\/opt\/homebrew\/bin\/nvim",
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "gitlens.mode.statusBar.enabled": false,
  "gitlens.menus": false,
  "gitlens.defaultDateStyle": "absolute",
  "gitlens.terminalLinks.enabled": false,
  "gitlens.currentLine.enabled": false,
  "go.gopath": "/Users/hushyael/code/go",
  "go.terminal.activateEnvironment": false,
  "go.coverOnTestPackage": false,
  "go.formatTool": "goimports",
  "go.lintTool": "golint",
  "go.lintFlags": ["-checks=all"],
  "gopls": {
    "ui.diagnostic.staticcheck": true,
    "ui.completion.usePlaceholders": true,
  },
  "protoc": {
    "options": [
      "--proto_path=api/google/api"
    ]
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
  }
}
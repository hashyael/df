{
  "editor.fontFamily": "MonoLisa",
  "editor.accessibilitySupport": "off",
  "editor.wordWrap": "on",
  "editor.inlayHints.enabled": "on",
  "editor.fontSize": 13,
  "editor.tabSize": 2,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.insertSpaces": true,
  "editor.smoothScrolling": true,
  "editor.scrollBeyondLastLine": true,
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.formatOnSave": false,
  "editor.bracketPairColorization.enabled": false,
  "editor.guides.indentation": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "workbench.list.openMode": "doubleClick",
  "workbench.startupEditor": "none",
  "workbench.fontAliasing": "antialiased",
  "workbench.colorTheme": "mlia dark",
  "workbench.list.horizontalScrolling": true,
  "workbench.editor.enablePreview": false,
  "workbench.layoutControl.enabled": false,
  "workbench.activityBar.visible": false,
  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.enablePersistentSessions": false,
  "vscode-neovim.neovimInitVimPaths.darwin": "~\/.config\/nvim\/init.vim",
  "vscode-neovim.neovimExecutablePaths.darwin": "\/opt\/homebrew\/bin\/nvim",
  "vscode-neovim.useCtrlKeysForInsertMode": true,
  "vscode-neovim.useCtrlKeysForNormalMode": true,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "go.terminal.activateEnvironment": false,
  "go.toolsManagement.autoUpdate": true,
  "[go]": {
    "editor.tabSize": 4
  },
  "gopls": {
    "build.experimentalWorkspaceModule": true
  },
}